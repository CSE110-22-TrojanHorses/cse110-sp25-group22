{
  "name": "cse110-sp25-group22",
  "version": "1.0.0",
  "description": "[View our Team page](admin/team.md)",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@eslint/css": "^0.7.0",
    "@eslint/js": "^9.26.0",
    "babel-plugin-istanbul": "^5.2.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "jest-puppeteer": "^11.0.0",
    "jest-puppeteer-istanbul": "^0.5.3",
    "jsdoc": "^4.0.4",
    "parcel": "^2.15.1",
    "prettier": "3.5.3",
    "puppeteer": "^24.9.0"
  },
  "scripts": {
    "lint": "eslint --ext .js --ignore-pattern .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier source --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js __tests__/unit.test.js",
    "start": "parcel test.html --port 9999",
    "build": "parcel build test.html",
    "test:e2e": "jest __tests__/test.test.js --maxWorkers=1",
    "test:all": "npm run test:unit && npm run test:e2e"
  },
  "type": "module"
}
